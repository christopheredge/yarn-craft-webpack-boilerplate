{
  "name": "craft-yarn-webpack-project-boilerplate",
  "version": "1.0.0",
  "description": "MilesHerndon's CraftCMS, Yarn, & Webpack Project Boilerplate",
  "main": "index.js",
  "scripts": {
    "ds_store": "find . -name '.DS_Store' -type f -delete",
    "clean": "npm run ds_store && rimraf public/build",
    "watch": "npm run clean && WEBPACK_CONFIG_TYPE='watch' webpack --progress --colors",
    "build": "npm run clean && WEBPACK_CONFIG_TYPE='build' webpack -p --progress --colors",
    "launch": "npm run clean && WEBPACK_CONFIG_TYPE='launch' webpack -p --progress --colors"
  },
  "repository": "git@github.com:christopheredge/yarn-craft-webpack-boilerplate.git",
  "author": "MilesHerndon",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "favicons-webpack-plugin": "^0.0.7",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "rimraf": "^2.6.1",
    "robotstxt-webpack-plugin": "^2.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.6",
    "webpack-merge": "^4.1.0",
    "webpack-notifier": "^1.5.0"
  },
  "dependencies": {
    "jquery": "^3.2.1",
    "picturefill": "^3.0.2",
    "svg4everybody": "^2.1.9"
  }
}
